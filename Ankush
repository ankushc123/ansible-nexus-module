pipeline {
    agent any
    
    environment {
   PATH = "/opt/apache-maven-3.8.6/bin/:$PATH"
   }
    stages{
        stage('Git CheckOut')
        {
            steps{
                git 'https://github.com/ankushc123/ansible-nexus-module.git' // 
                echo "Git Checkout Completed"
       
            }
        }
         stage('Unit Test'){
                steps{
                    sh 'mvn test'
                }
            }
            stage('Build The Code'){
            steps{
                sh 'mvn clean install -DskipTest'
               }
            }

